{
   "name": "lulu-server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@types/body-parser": "^1.19.2",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/helmet": "^4.0.0",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/node": "^16.11.10",
      "@types/slug": "^5.0.3",
      "@types/uuid": "^9.0.1",
      "node-dev": "^8.0.0",
      "nodemon": "^2.0.22",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "axios": "^1.3.5",
      "axios-concurrency": "^1.0.4",
      "bcrypt": "^5.1.0",
      "body-parser": "^1.19.1",
      "browserify-zlib": "^0.2.0",
      "class-validator": "^0.14.0",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.17.2",
      "helmet": "^6.0.1",
      "jsonwebtoken": "^9.0.0",
      "pg": "^8.11.0",
      "reflect-metadata": "^0.1.13",
      "slug": "^8.2.2",
      "sqlite3": "^5.0.2",
      "stripe": "^12.5.0",
      "typeorm": "0.3.12",
      "typeorm-extension": "^2.7.0",
      "uuid": "^9.0.0"
   },
   "scripts": {
      "start": "set debug=*&&--omit=dev && ts-node-dev --respawn --transpile-only ./src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs -d ./src/data-source.ts",
      "[__for TypeORNM migration": "",
      "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -d ./src/data-source.ts",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts ",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/data-source.ts",
      "schema:drop": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts schema:drop",
      "schema:sync": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts schema:sync",
      "migration:show": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts migration:show",
      "]__for TypeORM migration": "",
      "[__for TypeORNM seeding": "",
      "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
      "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
      "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
      "]__for TypeORM seeding": ""
   }
}
